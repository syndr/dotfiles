#!/bin/bash

# Output a random number!

number=$1


function lightbulb() {
    therandom=""
    
    for i in $(seq 1 $number); do
        arandom=$((RANDOM % 10))
        echo "BURT -> $arandom"
        therandom="$therandom$arandom"
    done
    
    echo "ğŸ’¡ $therandom ğŸ’¡"
}

lightbulb $number

